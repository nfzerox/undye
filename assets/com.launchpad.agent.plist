<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.launchpad.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/Shared/.launchpad/Launchpad</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/launchpad-agent.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/launchpad-agent.error.log</string>
</dict>
</plist>